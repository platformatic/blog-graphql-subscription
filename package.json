{
  "name": "blog-graphql-subscription",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/blog-graphql-subscription.git"
  },
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
